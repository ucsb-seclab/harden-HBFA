[Defines]
  INF_VERSION                    = 1.26
  BASE_NAME                      = stdlibhfunctionsPei
  MODULE_UNI_FILE                = stdlibhfunctionsPei.uni
  FILE_GUID                      = CDE000FF-FABB-436D-B4A3-18CC6CA7C746
